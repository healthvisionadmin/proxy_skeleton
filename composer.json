{
    "name": "based on mrcoco/slim3-eloquent-skeleton",
    "description": "Simple Slim Framework 3 skeleton with scaffold tool, migrations, behat testing",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": [
        "slim-framework", 
        "skeleton"
    ],
    "authors": [
        {
            "name": "knowledgeatwork",
            "email": "info@knowledgeatwork.eu",
            "homepage": "http://knowledgeatwork.eu"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Helpers\\": "migrations/helpers"
        }
    },
    "require": {
        "slim/slim": "^3.0",
        "monolog/monolog": "^1.17",
        "illuminate/database": "^5.4",
        "swiftmailer/swiftmailer": "@stable",
        "davidepastore/slim-validation": "^0.5.0",
		"awurth/slim-validation": "^2.0"
    },
    "require-dev": {
        "zeuxisoo/slim-whoops": "^0.4.0",
        "davedevelopment/phpmig": "^1.2",
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit": "^5.7",
        "guzzlehttp/guzzle": "~6",
        "namshi/cuzzle": "^2.0",
        "behat/behat": "~3.0"
    }
}
